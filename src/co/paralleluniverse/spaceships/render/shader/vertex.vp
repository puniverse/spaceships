#version 330 core

in vec2 in_Position;
in vec3 in_Vertex;

out vec4 pass_Color;
out float heading;
out float shootLength;

void main()
{
    gl_Position = vec4(in_Position, 1.0, 1.0);
//    gl_PointSize = 10;
    pass_Color = vec4(in_Vertex[0],0.0, 0.0, 1.0);
    heading = in_Vertex[1];
    shootLength = in_Vertex[2];
}